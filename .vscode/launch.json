{
  "version": "0.2.0",
  "configurations": [{
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/node_modules/@quasar/app/bin/quasar",
      "args": [
        "dev"
      ],
      "stopOnEntry": true
    },
    {
      "type": "node-terminal",
      "name": "Run Script: Kai Dev",
      "request": "launch",
      "command": "source ~/config.fish && quasar dev",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: Dev",
      "request": "launch",
      "command": "quasar dev",
      "cwd": "${workspaceFolder}"
    }
  ]
}
